<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hrbhd.bady.domain.Schedule" table="t_schedule"
		catalog="hdbady">
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="uuid" />
		</id>

		<!-- 1 -->
		<property name="grade" type="java.lang.String">
			<column name="grade" length="32" />
		</property>
		<property name="morning_has_num" type="java.lang.String">
			<column name="morning_has_num" length="32" />
		</property>
		<property name="morning_start" type="java.lang.String">
			<column name="morning_start" length="32" />
		</property>

		<!-- 2 -->
		<property name="afternoon_has_num" type="java.lang.String">
			<column name="afternoon_has_num" length="32" />
		</property>
		<property name="afternoon_start" type="java.lang.String">
			<column name="afternoon_start" length="32" />
		</property>
		<property name="duration" type="java.lang.String">
			<column name="duration" length="32" />
		</property>
		<!-- 3 -->
		<property name="rest_duration" type="java.lang.String">
			<column name="rest_duration" length="32" />
		</property>
		<property name="week_type_label" type="java.lang.String">
			<column name="week_type_label" length="32" />
		</property>
		<property name="edit" type="java.lang.String">
			<column name="edit" length="32" />
		</property>
		<!-- 4 -->
		<property name="start_time" type="java.lang.String">
			<column name="start_time" length="32" />
		</property>
		<property name="end_time" type="java.lang.String">
			<column name="end_time" length="32" />
		</property>
		<property name="dayNum" type="java.lang.String">
			<column name="dayNum" length="32" />
		</property>
		<!-- 5 -->
		<property name="start_end_time" type="java.lang.String">
			<column name="start_end_time" length="32" />
		</property>
	</class>
</hibernate-mapping>